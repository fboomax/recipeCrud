    "pk": 94,
    "fields": {
      "recipe": 6,
      "step": 1,
      "title": "Χοντροκόβουμε",
      "description": "Χοντροκόβουμε στον πολυκόφτη ή με ένα κοφτερό μαχαίρι την καρδιά και το πνευμόνι.",
      "duration": 3.0,
      "image": "",
      "updated": "2023-01-29T14:22:19.450Z",
      "created": "2023-01-29T14:22:19.447Z"
